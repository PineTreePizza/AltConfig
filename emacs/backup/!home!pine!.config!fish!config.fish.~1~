function fish_prompt
    set -l textcol  black
    set -l bgcol    blue
    set -l arrowcol blue
    set_color $textcol -b $bgcol
    echo -n " "(basename $PWD)" "
    set_color $arrowcol -b normal
    echo -n "â–¶ "
end
set fish_greeting
set -Ux QT_QPA_PLATFORMTHEME qt6ct